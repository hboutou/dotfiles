<?xml version="1.0" encoding="UTF-8"?>
<settings>
	<mirrors>
		<mirror>
			<mirrorOf>*</mirrorOf>
			<url>https://artifacts.example.com/repository/maven-dev-group/</url>
			<id>mirror-all</id>
		</mirror>
	</mirrors>
	<servers>
		<server>
			<id>maven-dev-snapshot-hosted</id>
			<username></username>
			<password></password>
		</server>
		<server>
			<id>maven-dev-release-hosted</id>
			<username></username>
			<password></password>
		</server>
	</servers>
	<profiles>
		<profile>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<repositories>
				<repository>
					<releases />
					<snapshots />
					<id>id</id>
					<url>url</url>
				</repository>
			</repositories>
		</profile>
		<profile>
			<id>SUREFIRE-1588</id>
			<activation><activeByDefault>true</activeByDefault></activation>
			<properties><argLine>-Djdk.net.URLClassPath.disableClassPathURLCheck=true</argLine></properties>
		</profile>
		<profile>
			<id>AWS-SDK1-ANNOYANCE</id>
			<activation><activeByDefault>true</activeByDefault></activation>
			<properties><argLine>-Daws.java.v1.disableDeprecationAnnouncement=true</argLine></properties>
		</profile>
	</profiles>
</settings>

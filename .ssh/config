Host *
    ControlMaster auto
    ControlPath ~/.ssh/master-%r@%h:%p
    ControlPersist 600
    ServerAliveInterval 120
    ServerAliveCountMax 5

Host really-old-server
    IdentityFile ~/.ssh/id_rsa
    PubkeyAcceptedKeyTypes +ssh-rsa

# ProxyJump maingate
# UserKnownHostsFile /dev/null
# StrictHostKeyChecking no

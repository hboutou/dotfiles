Host *
    ControlMaster auto
    ControlPath ~/.ssh/%r@%h:%p.socket
    ControlPersist 600
    ServerAliveInterval 120
    ServerAliveCountMax 5

Host really-old-server
    IdentityFile ~/.ssh/id_rsa
    PubkeyAcceptedKeyTypes +ssh-rsa

# ProxyJump maingate
# UserKnownHostsFile /dev/null
# StrictHostKeyChecking no

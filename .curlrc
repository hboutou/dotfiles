output-dir ~/Downloads
create-dirs
retry 3
show-error
silent

# fake agent Edge/Win10
user-agent = Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36 Edg/120.0.2210.144

# allow no less than 10 bytes/sec during a 3-second interval
speed-limit 10
speed-time 3
